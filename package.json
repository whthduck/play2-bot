{
  "name": "@whthduck/play2-bot",
  "version": "0.0.10",
  "description": "Joins voice channels and all that stuff",
  "author": "whthduck",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "dev": "nest start --watch",
    "prod": "node dist/main",
    "image:podman": "podman build -t play2-bot . && podman push play2-bot whthduck/play2-bot:${npm_package_version} && podman push play2-bot whthduck/play2-bot:latest",
    "image:docker": "docker build -t play2-bot . && docker push play2-bot whthduck/play2-bot:${npm_package_version} && docker push play2-bot whthduck/play2-bot:latest",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@discord-nestjs/common": "~5.2",
    "@discord-nestjs/core": "~5.3",
    "@discordjs/opus": "~0.9",
    "@discordjs/voice": "~0.15",
    "@nestjs/bull": "~0.6",
    "@nestjs/common": "~9.0",
    "@nestjs/config": "~2.3",
    "@nestjs/core": "~9.0",
    "@nestjs/event-emitter": "~1.4",
    "@nestjs/platform-express": "~9.0",
    "@nestjs/platform-fastify": "~9.3",
    "@types/uuid": "^9.0.1",
    "bull": "~4.10",
    "cache-manager": "^5.1.7",
    "cache-manager-ioredis-yet": "^1.1.0",
    "class-transformer": "~0.5",
    "class-validator": "~0.14",
    "discord.js": "~14.8",
    "fastify": "~4.14",
    "flat": "^5.0.2",
    "libsodium-wrappers": "~0.7",
    "nestjs-pino": "^3.1.2",
    "pino-http": "^8.3.3",
    "play-dl": "~1.9",
    "redis": "~3.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.13",
    "@types/flat": "^5.0.2",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
